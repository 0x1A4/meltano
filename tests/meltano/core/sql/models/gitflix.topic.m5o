{
  version = 1
  name = gitflix
  connection = runners_db
  label = GitFlix Example
  designs {
    users {
      label = HDA Example
      from = users_table
      description = Base for Hyper Dymensional Aggregates
      joins {
	streams {
	  label = Streams
	  from = streams_table
	  sql_on = "users.id = streams.user_id"
	  relationship = many_to_one
	}
	episodes {
	  label = Episodes
	  from = episodes_table
	  sql_on = "episodes.id = streams.episode_id"
	  relationship = many_to_one
	}
      }
    }

    streams {
      label = Streams
      from = streams_table
      description = HDA from Streams
      joins {
	users {
	  label = Users
	  from = users_table
	  sql_on = "streams.user_id = users.id"
	  relationship = many_to_one
	}
	episodes {
	  label = Episodes
	  from = episodes_table
	  sql_on = "streams.episode_id = episodes.id"
	  relationship = many_to_one
	}
      }
    }
  }
}
